{
  "name": "nestjs-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "author": "≈Åukasz Jeziorski",
  "license": "MIT",
  "scripts": {
    "apiDocs:open": "open-cli http://localhost:3000/docs",
    "build": "nest build",
    "docker:build": "docker-compose up -d -V --build",
    "docker:down": "docker-compose down",
    "doc": "npx @compodoc/compodoc -p tsconfig.json -s --theme 'material'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "init": "npm run docker:build && npm run push:run && npm run seed:run && npm run mailClient:open && npm run apiDocs:open && npm run prisma:studio",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "mailClient:open": "open-cli http://localhost:1080/",
    "prepare": "is-ci || husky",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "push:run": "npx prisma db push",
    "seed:run": "npx prisma db seed",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.13",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.13",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.13",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.13",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.2.1",
    "@onesignal/node-onesignal": "^5.0.0-alpha-01",
    "@prisma/client": "^5.10.2",
    "@types/multer": "^1.4.11",
    "apple-signin-auth": "^1.7.6",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "eslint-plugin-import": "^2.29.1",
    "fb": "^2.0.0",
    "google-play-scraper": "^9.2.0",
    "googleapis": "^134.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "husky": "^9.0.11",
    "ioredis": "^5.3.2",
    "is-ci": "^3.0.1",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^10.5.0",
    "nodemailer": "^6.9.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.13",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.17",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "ms": "^2.1.3",
    "open-cli": "^8.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "~starter/config/(.*)$": "<rootDir>/config/$1",
      "~starter/consent/(.*)$": "<rootDir>/consent/$1",
      "~starter/modules/(.*)$": "<rootDir>/modules/$1",
      "~starter/providers/(.*)$": "<rootDir>/providers/$1",
      "~starter/roles/(.*)$": "<rootDir>/roles/$1",
      "~starter/session/(.*)$": "<rootDir>/session/$1",
      "~starter/shared/(.*)$": "<rootDir>/shared/$1",
      "~starter/system/(.*)$": "<rootDir>/system/$1",
      "~starter/social/(.*)$": "<rootDir>/social/$1",
      "~starter/statuses/(.*)$": "<rootDir>/statuses/$1",
      "~starter/translations/(.*)$": "<rootDir>/translations/$1",
      "~starter/utils/(.*)$": "<rootDir>/utils/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "src/database/schema.prisma",
    "seed": "ts-node src/database/seed.ts"
  }
}
